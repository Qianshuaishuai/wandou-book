page{
  position: relative;
}

.btn {
  width: 50%;
  height: 90rpx;
  margin-top: 50rpx;
  background-color: #38b08f;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border: none;
}

.btn::after {
  border: none;
}

.img-class{
  display: flex;
  justify-content: center
}


.btn-new {
  width: 50%;
  height: 90rpx;
  margin-top: 50rpx;
  background-color: #38b08f;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border: none;
  position: absolute;
  bottom: 24rpx;
  left: 25%;
}

.btn-new::after {
  border: none;
}
