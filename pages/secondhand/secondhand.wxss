page {
  background: #f4f4f9;
  padding: 24rpx;
  padding-bottom: 168rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.show-list {
  display: flex;
  flex-direction: column;
}

.show-list .show-block {
  width: 100%;
  background: #fff;
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}

.show-list .show-block .title {
  margin-left: 36rpx;
  margin-right: 36rpx;
  margin-top: 32rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.show-list .show-block .price-box {
  display: flex;
  flex-direction: row;
  margin-left: 37rpx;
  align-items: flex-end;
  margin-top: 30rpx;
}

.show-list .show-block .price-box .tip {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #fa3434;
}

.show-list .show-block .price-box .price {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #fa3434;
}

.show-list .show-block .price-box .old-price {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  text-decoration: line-through;
  color: #aaa;
  margin-left: 6rpx;
}

.show-list .show-block .photo-list {
  display: flex;
  flex-direction: row;
  display: -webkit-box;
  overflow-x: auto;
  -webkit-overflow-scrolling: touch;
  margin-right: 30rpx;
  margin-left: 10rpx;
  overflow-y: hidden;
  margin-top: 36rpx;
}

.show-list .show-block .photo-list .photo-block {
  width: 200rpx;
  height: 200rpx;
  margin-left: 20rpx;
  border-radius: 12rpx;
}

.show-list .show-block .photo-list .photo-block .photo {
  width: 200rpx;
  height: 200rpx;
  border-radius: 12rpx;
}

.show-list .show-block .note {
  margin-top: 28rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  background: #f4f4f9;
  border-radius: 10rpx;
  padding: 28rpx 28rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #6b6b7d;
}

.show-list .show-block .bottom {
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-top: 24rpx;
  margin-bottom: 28rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.show-list .show-block .bottom .left {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.show-list .show-block .bottom .left .icon1 {
  width: 21rpx;
  height: 26rpx;
}

.show-list .show-block .bottom .left .address {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #6b6b7d;
  margin-left: 12rpx;
}

.show-list .show-block .bottom .button {
  width: 128rpx;
  height: 44rpx;
  position: relative;
}

.show-list .show-block .bottom .button .bg {
  width: 128rpx;
  height: 44rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.show-list .show-block .bottom .button .tip2 {
  width: 128rpx;
  height: 44rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  text-align: center;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #fff;
  padding-top: 6rpx;
}

.search-box {
  display: flex;
  flex-direction: row;
  position: relative;
  margin-top: 25rpx;
  height: 70rpx;
  background: #fff;
  border-radius: 35rpx;
}

.search-box .icon1 {
  width: 28rpx;
  height: 28rpx;
  left: 36rpx;
  top: 21rpx;
  position: absolute;
}

.search-box .search {
  width: 206rpx;
  height: 70rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #b7b7b7;
  line-height: 70rpx;
  padding-left: 78rpx;
}

.search-box .button2 {
  width: 150rpx;
  height: 44rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  z-index: 11;
}

.search-box .button2 .bg2 {
  width: 150rpx;
  height: 70rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  z-index: 12;
}

.search-box .button2 .tip12 {
  width: 150rpx;
  height: 70rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  z-index: 13;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #fff;
  line-height: 70rpx;
  text-align: center;
}

.box {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 32rpx;
  margin-top: 36rpx;
  display: -webkit-box;
  overflow-x: scroll;
  -webkit-overflow-scrolling: touch;
}

.box .block {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 140rpx;
  height: 80rpx;
  justify-content: center;
}

.box .block .tip10 {
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #0687f7;
}

.box .block .diviler {
  width: 100rpx;
  height: 4rpx;
  margin-top: 20rpx;
  background: #0687f7;
}

::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}

.dialog2 {
  width: 530rpx;
  height: 324rpx;
  background: #fff;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  top: 35%;
  left: 50%;
  margin-left: -265rpx;
  z-index: 9999999;
}

.dialog2 .pic2 {
  width: 99rpx;
  height: 80rpx;
  margin-top: 58rpx;
}

.dialog2 .tip11 {
  width: 304rpx;
  height: 60rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333;
  margin-top: 36rpx;
  text-align: center;
}

.dialog-bg {
  width: 100%;
  height: 100vh;
  opacity: 0.5;
  background: #000;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999999;
}

