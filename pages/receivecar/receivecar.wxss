.list .block .box2 {
  position: absolute;
  bottom: 18rpx;
  right: 12rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.list .block .box2 .do {
  width: 42rpx;
  height: 42rpx;
}

.list .block .box2 .count {
  margin-left: 32rpx;
  margin-right: 32rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333;
}

.util {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding-left: 32rpx;
  padding-right: 32rpx;
  height: 100rpx;
  background: #fff;
  border-radius: 12rpx;
  margin-top: 20rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
}

.util .left {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.util .left .tip6 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
}

.util .left .question {
  margin-left: 20rpx;
  width: 32rpx;
  height: 32rpx;
}

.util .filter {
  width: 94rpx;
  height: 44rpx;
}

.button-box {
  margin-top: 48rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-end;
}

.button-box .button {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.button-box .button .icon1 {
  width: 128rpx;
  height: 128rpx;
}

.button-box .button .icon2 {
  width: 160rpx;
  height: 175rpx;
  margin-left: 80rpx;
  margin-right: 80rpx;
}

.button-box .button .icon3 {
  width: 128rpx;
  height: 128rpx;
}

.button-box .button .tip7 {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
  margin-top: 20rpx;
}

.dialog-bg {
  width: 100%;
  height: 100vh;
  opacity: 0.5;
  background: #000;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999999;
}

.dialog-tip {
  width: 530rpx;
  height: 280rpx;
  background: #fff;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  top: 38%;
  left: 50%;
  margin-left: -280rpx;
  z-index: 9999999;
  justify-content: center;
  text-align: center;
  padding-left: 24rpx;
  padding-right: 24rpx;
}

.box {
  font-weight: bold;
  font-size: 40upx;
  white-space: nowrap;
  width: 200upx;
  height: 80upx;
  position: relative;
  overflow: hidden;
  /* background-color: aquamarine; */
}

.wp{
  width: 100%;
  margin-right: 24rpx;
  margin-left: 18rpx;
}

.text {
  color: #000;
  height: 80upx;
  display: flex;
  align-items: center;
  position: absolute;
  line-height: 1em;
}

.box{
  position: relative;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
}

.txt{
  white-space: nowrap;
}

#txt1{
  position: relative;
  white-space: nowrap;
  overflow: hidden;
  transition: left 1s linear;
}

#txt2,.flex-box{
  position: absolute;
  top: 0;
}

.flex-box{
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

@keyframes roll {
  0% {left: 750rpx;}
  100% {left: -100%;}
}