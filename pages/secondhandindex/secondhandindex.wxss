  left: 3%;
}

.search-box .search {
  width: 27rpx;
  height: 27rpx;
  position: absolute;
  top: 28rpx;
  left: 26rpx;
}

.search-box .delete {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 28rpx;
  right: 26rpx;
  z-index: 999;
}

.search-box .input {
  width: 100%;
  height: 100%;
  padding-left: 68rpx;
  line-height: 88rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #a7a7a7;
}

.tip-title {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
  z-index: 778rpx;
  position: fixed;
  top: 596rpx;
  left: 30rpx;
}

.type-list {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
  padding-left: 30rpx;
