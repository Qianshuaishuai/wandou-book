  display: flex;
  flex-direction: column;
  background: #f4f4f9;
  padding-bottom: 168rpx;
}

.bg {
  width: 100%;
  height: 536rpx;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 1;
}

.top {
  width: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
}

