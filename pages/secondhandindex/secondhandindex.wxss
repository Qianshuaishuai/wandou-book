  border-radius: 23rpx;
  padding: 12rpx 24rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #7d7d7d;
  margin-top: 24rpx;
  margin-left: 20rpx;
}

.middle-bg {
  position: fixed;
  top: 536rpx;
  width: 100%;
  height: 520rpx;
  background: #fff;
  z-index: 777rpx;
}

.button-box {
