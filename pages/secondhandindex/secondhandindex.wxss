.back {
  width: 34rpx;
  height: 34rpx;
  position: fixed;
  left: 30rpx;
  z-index: 4;
}

.tip {
  width: 100%;
  height: 34rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #fff;
  position: fixed;
  text-align: center;
  z-index: 2;
}

.search-box {
  width: 94%;
  height: 88rpx;
  border-radius: 48rpx;
  background: #fff;
