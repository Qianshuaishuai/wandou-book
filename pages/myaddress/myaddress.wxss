/* pages/myaddress/myaddress.wxss */
page {
  background: #f4f4f9;
  padding-bottom: 168rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.tip {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #85858D;
  margin-top: 24rpx;
  margin-left: 27rpx;
  margin-bottom: 24rpx;
}

.list {
  display: flex;
  flex-direction: column;
  margin-left: 27rpx;
  margin-right: 27rpx;
}

.list .block {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 250rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  margin-bottom: 20rpx;
}

.list .block .content {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 28rpx;
}

.list .block .content .icon {
  margin-left: 25rpx;
  width: 48rpx;
  height: 48rpx;
  margin-right: 26rpx;
}

.list .block .content .box {
  display: flex;
  flex-direction: column;
}

.list .block .content .box .box1 {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.list .block .content .box .box1 .name {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.list .block .content .box .box1 .phone {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #605F5F;
  margin-left: 24rpx;
}

.list .block .content .box .box1 .tag {
  margin-left: 22rpx;
  background: rgba(24, 155, 251, 0);
  border: 1px solid #0A8BF8;
  border-radius: 5rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #0A8BF8;
  line-height: 28rpx;
  width: 60rpx;
  height: 28rpx;
  text-align: center;
}

.list .block .content .box .address {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  margin-top: 16rpx;
}

.list .block .diviler {
  margin-left: 25rpx;
  margin-right: 25rpx;
  height: 1px;
  background: #E6E6E6;
  margin-top: 28rpx;
  margin-bottom: 16rpx;
}

.list .block .button {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 25rpx;
  margin-right: 25rpx;
}

.list .block .button .default {
  width: 150rpx;
  height: 50rpx;
  background: rgba(37, 134, 255, 0);
  border: 1px solid #B7B7B7;
  border-radius: 25rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #A3A3A3;
  line-height: 50rpx;
  text-align: center;
}

.list .block .button .right {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.list .block .button .right .delete {
  width: 150rpx;
  height: 50rpx;
  background: rgba(37, 134, 255, 0);
  border: 1px solid #B7B7B7;
  border-radius: 25rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #A3A3A3;
  line-height: 50rpx;
  text-align: center;
}

.list .block .button .right .edit {
  width: 150rpx;
  height: 50rpx;
  background: rgba(37, 134, 255, 0);
  border: 1px solid #B7B7B7;
  border-radius: 25rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #A3A3A3;
  line-height: 50rpx;
  text-align: center;
  margin-left: 20rpx;
}

.build {
  margin-top: 96rpx;
  margin-left: 66rpx;
  margin-right: 66rpx;
  height: 110rpx;
  position: relative;
}

.build .bg {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: 3;
}

.build .box3 {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100%;
  z-index: 4;
  justify-content: center;
  padding-top: 26rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.build .box3 .add {
  width: 28rpx;
  height: 28rpx;
  margin-top: 6rpx;
}

.build .box3 .tip3 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
  margin-left: 16rpx;
}