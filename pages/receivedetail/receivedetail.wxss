page {
  background: #f4f4f9;
  padding: 24rpx;
  padding-bottom: 168rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.bg {
  width: 100%;
  height: 308rpx;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 1;
}

.top {
  width: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
}

