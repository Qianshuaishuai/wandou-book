page {
  background: #f4f4f9;
  padding-bottom: 168rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.main {
  display: flex;
  flex-direction: column;
  position: relative;
  padding-bottom: 128rpx;
  box-sizing: border-box;
}

.main .bg {
  width: 100%;
  height: 308rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: 1;
}

.main .top {
  width: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
}

/* .top .title {
  width: 100%;
  z-index: 2;
  display: flex;
  flex-direction: row;
  position: relative;
  height: 34rpx;
} */

.main .back {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  left: 30rpx;
  z-index: 10;
}

.main .tip {
  width: 100%;
  height: 34rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #fff;
  position: absolute;
  text-align: center;
  z-index: 2;
}

.content {
  display: flex;
  flex-direction: column;
  position: relative;
  z-index: 999;
  margin-top: 180rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
  box-sizing: border-box;
}

.content .bg1 {
  width: 100%;
  height: 20rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.content .bg2 {
  width: 100%;
  height: 1380rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.content .title {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #020615;
  margin-left: 62rpx;
  margin-top: 36rpx;
  z-index: 1000;
}

.content .address-box {
  width: 600rpx;
  height: 160rpx;
  background: #f4f5f9;
  border-radius: 12rpx;
  margin-top: 36rpx;
  align-self: center;
  z-index: 1000;
  display: flex;
  flex-direction: column;
}

.content .address-box .address-block {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 26rpx;
}

.content .address-box .address-block .icon {
  width: 36rpx;
  height: 36rpx;
  margin-left: 25rpx;
}

.content .address-box .address-block .address {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
  margin-left: 27rpx;
}

.content .address-box .middle {
  width: 3rpx;
  height: 32rpx;
}

.content .express {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #6d6d6d;
  margin-top: 24rpx;
  z-index: 1002;
  margin-left: 62rpx;
}

.button-box {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  z-index: 2222;
  margin-top: 144rpx;
  align-self: center;
  margin-left: 24rpx;
  margin-right: 24rpx;
  box-sizing: border-box;
}

.button-box .button {
  width: 337rpx;
  height: 109rpx;
  position: relative;
}

.button-box .button .bg {
  width: 337rpx;
  height: 109rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: 2223;
}

.button-box .button .box {
  width: 337rpx;
  height: 109rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  display: flex;
  flex-direction: row;
  padding-top: 20rpx;
  justify-content: center;
  z-index: 2224;
}

.button-box .button .box .icon {
  margin-top: 5rpx;
  width: 36rpx;
  height: 36rpx;
}

.button-box .button .box .tip5 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #fff;
  margin-left: 16rpx;
}

.dialog-bg {
  width: 100%;
  height: 100vh;
  opacity: 0.5;
  background: #000;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999999;
}

.dialog2 {
  width: 530rpx;
  height: 368rpx;
  background: #fff;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  top: 35%;
  left: 50%;
  margin-left: -265rpx;
  z-index: 9999999;
}

.dialog2 .tip15 {
  width: 530rpx;
  height: 60rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #0080f6;
  text-align: center;
  margin-top: 24rpx;
}

.dialog2 .button {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
}

.dialog2 .button .cancel {
  width: 50%;
  height: 80rpx;
  text-align: center;
  line-height: 84rpx;
  font-size: 28rpx;
  border-top: 1rpx solid #BEBEBE;
  border-right: 1rpx solid #BEBEBE;
}

.dialog2 .button .send {
  width: 50%;
  height: 80rpx;
  text-align: center;
  line-height: 84rpx;
  font-size: 28rpx;
  color: #0080f6;
  border-top: 1rpx solid #BEBEBE;
}

.dialog2 .input {
  margin-top: 24rpx;
  padding-left: 150rpx;
  padding-right: 150rpx;
  width: 90%;
}

.content .photo-box {
  display: flex;
  flex-direction: row;
  display: -webkit-box;
  overflow-x: auto;
  -webkit-overflow-scrolling: touch;
  overflow-y: hidden;
  margin-left: 42rpx;
  margin-right: 42rpx;
  margin-bottom: 24rpx;
}

::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}

.content .photo-box .photo-block {
  width: 200rpx;
  height: 200rpx;
  margin-left: 10rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 12rpx;
}

.content .photo-box .photo-block .show {
  width: 200rpx;
  height: 200rpx;
  position: relative;
  border-radius: 12rpx;
}

.content .photo-box .photo-block .show .delete {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
}

.content .photo-box .photo-block .show .pic {
  width: 180rpx;
  height: 180rpx;
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  border-radius: 12rpx;
}

.content .photo-box .photo-block .add {
  width: 180rpx;
  height: 180rpx;
  margin-top: 20rpx;
  border-radius: 12rpx;
  border: 1rpx dashed #a8a8a8;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.content .photo-box .photo-block .add .icon {
  width: 60rpx;
  height: 60rpx;
  margin-top: 32rpx;
}

.content .photo-box .photo-block .add .tip5 {
  margin-top: 18rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #a8a8a8;
}

.show-pic {
  position: fixed;
  z-index: 9999999;
}